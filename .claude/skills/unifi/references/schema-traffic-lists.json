            "$ref": "#/components/schemas/Create or update traffic matching list"
          },
          {
            "type": "object",
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/IPv4 matching"
                },
                "minItems": 1
              }
            }
          }
        ],
        "required": [
          "items",
          "name"
        ]
      },
      "IntegrationIpV4TrafficMatchingListDto": {
        "allOf": [
