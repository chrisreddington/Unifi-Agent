      "Traffic matching lists page": {
        "properties": {
          "offset": {
            "type": "integer",
            "format": "int64",
            "example": 0
          },
          "limit": {
            "type": "integer",
            "format": "int32",
            "example": 25
          },
          "count": {
            "type": "integer",
            "format": "int32",
            "example": 10
          },
          "totalCount": {
            "type": "integer",
            "format": "int64",
            "example": 1000
          },
          "data": {
            "type": "array",
            "items": {
