          {
            "type": "object",
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/Port matching"
                },
                "minItems": 1
              }
            }
          }
        ],
        "required": [
          "items",
          "name"
        ]
      },
      "IntegrationPortTrafficMatchingListDto": {
        "allOf": [
          {
            "$ref": "#/components/schemas/Traffic matching list"
          },
          {
            "type": "object",
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/Port matching"
                },
                "minItems": 1
              }
            }
          }
        ],
        "required": [
          "id",
          "items",
