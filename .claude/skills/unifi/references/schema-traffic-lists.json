      "Number port matching": {
        "allOf": [
          {
            "$ref": "#/components/schemas/Port matching"
          },
          {
            "type": "object",
            "properties": {
              "value": {
                "type": "integer",
                "format": "int32",
                "description": "Port number",
                "example": "80|443|8080",
